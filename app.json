{
    "name": "Convert GA4 data to Snowplow events",
    "env": {
        "ga4_dataset": {
            "description": "Dataset where GA4 is",
            "value": "analytics_341844832",
            "required": false
        },
        "ga4_table": {
            "description": "GA4 table (default is all tables)",
            "value": "events_*",
            "required": false
        },
        "destination_dataset_and_table": {
            "description": "e.g. dataset.table - dataset must exist",
            "value": "ga4_to_snowplow.ga4_events"
        },
        "start_date": {
            "description": "Date you want to query events from, in format 20220101",
            "value": "",
            "required": false
        },
        "end_date": {
            "description": "Date you want to query events until, in format 20220101",
            "value": "",
            "required": false
        }
    }
}